#include <stdio.h>
void main ()
{
  char str[20];
  printf ("enter the strings\t");
  scanf ("%s", str);

  printf ("%s", str);
  if (str[0] =='/' && str[1]=='*'|| str[0] =='/' && str[1]=='/')
    {
      printf ("it is a comment");
     
    }
  else
    {
      printf ("it is a not a comment");

    }
    
  getch ();
}
